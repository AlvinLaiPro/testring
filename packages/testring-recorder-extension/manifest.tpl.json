{
  "manifest_version": 2,
  "name": "TestRing",
  "author": "RingCentral",
  "icons": {
    "48": "icon.png"
  },
  "page_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs"
  ]
}
