{
  "name": "@testring/e2e-test-app",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ringcentral/testring.git"
  },
  "author": "RingCentral",
  "license": "MIT",
  "scripts": {
    "test:selenium": "testring --config=test/selenium/config.json",
