{
  "name": "@testring/e2e-test-app",
  "private": true,
  "version": "0.2.13",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ringcentral/testring.git"
  },
  "author": "RingCentral",
  "license": "MIT",
  "scripts": {
    "test:selenium": "testring run --config ./test/selenium/config.js --log-level debug",
    "test:simple-run": "testring run --config test/simple/.testringrc",
    "test:run-and-record": "testring record --config test/simple/config.json --tests test/simple/*.spec.js",
    "e2e-simple": "npm run test:simple-run",
    "e2e": "npm run test:simple-run && npm run test:selenium"
  },
  "dependencies": {
    "@testring/plugin-babel": "0.2.13",
    "@testring/plugin-selenium-driver": "0.2.13",
    "babel-preset-es2015": "^6.24.1",
    "selenium-server": "^3.13.0",
    "testring": "0.2.13"
  }
}
